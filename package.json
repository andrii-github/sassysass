{
  "name": "sassysass",
  "version": "1.3.1",
  "description": "Sassy Sass is a CSS architecture scaffolding for Sass.",
  "main": "bin/sassysass",
  "preferGlobal": true,
  "bin": {
    "sassysass": "./bin/sassysass"
  },
  "scripts": {
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanburgess/sassysass.git"
  },
  "keywords": [
    "sass",
    "scss",
    "sassysass",
    "boilerplate",
    "scaffolding",
    "build",
    "architecture",
    "smacss",
    "methodology"
  ],
  "authors": [
    "Ryan Burgess @burgessdryan",
    "Wai Lun Poon @monkeytypewritr",
    "Augustus Yuan @augburto"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanburgess/sassysass/issues"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^1.5.0",
    "fs-extra": "^0.16.5",
    "get-date": "^1.0.2",
    "install": "^0.1.8",
    "mkdirp": "^0.5.1",
    "npm": "^2.14.15",
    "path": "^0.11.14",
    "prepend-file": "^1.3.0",
    "prompt": "^0.2.14",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta4",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "ghooks": "^1.0.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.1",
    "gulp-jsonlint": "^1.1.1",
    "gulp-nodeunit": "0.0.5",
    "https": "^1.0.0",
    "jshint": "^2.9.1-rc1",
    "jshint-stylish": "^2.1.0",
    "tape": "^4.2.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "gulp lint",
      "pre-push": "gulp test",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "peerDependencies": {},
  "homepage": "https://github.com/ryanburgess/sassysass#readme"
